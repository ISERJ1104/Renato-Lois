algoritimo "12"
var
	a : caracter
  b : caracter
  auxiliar : caracter
inicio
	escreval("digite um valor para a variavel [a]")
 		leia(a)
  escreval("digite um valor para a variavel [b]")
  	leia(b]
	   auxiliar <- a
	   a <- b
	   b <- auxiliar
  escreval("os valores, invertidos, ficaram: [a] = ", a, " e [b] = ", b)
fimalgoritimo
