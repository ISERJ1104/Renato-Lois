algoritmo "22"
var
	a : real
 	b : real
  c : real
inicio
	escreval("Digite o valor por hora de trabalho")
 		leia(a)
  escreval("digite a quantidade de horas")
  	leia(b)
  escreval("digite o percentual de disconto do INSS")
  	leia(c)
  escreval("seu salario liquido e: " (a * b) - (a * b * c / 100)
fimalgoritmo
